{
  "name": "Example PackageJson",
  "version": "1.0.0",
  "private": true,
  "main": "./app.js",
  "eslintConfig": {
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "plugins": [
      "html"
    ],
    "extends": [
      "frontier"
    ],
    "rules": {
      "valid-jsdoc": "off"
    },
    "globals": {
      "Polymer": true,
      "ReduxMixin": true,
      "url": true,
      "semverLite": true
    }
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-frontier": "github:fs-webdev/eslint-config-frontier",
    "eslint-plugin-html": "^4.0.2",
    "mocha": "^5.0.0"
  },
  "engines": {
    "node": "8"
  },
  "scripts": {
    "test": "mocha"
  }
}
